{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:e2e": "dotenv -e .env.test jest --no-cache --runInBand --expose-gc",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:up": "docker compose up dev_db -d",
    "db:dev:rm": "docker compose rm dev_db -s -f -v",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && delay 1 && yarn prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test prisma migrate deploy",
    "db:test:up": "docker compose up test_db -d",
    "db:test:rm": "docker compose rm test_db -s -f -v",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && delay 1 && yarn prisma:test:deploy",
    "services:start": "docker compose up -d",
    "services:stop": "docker compose down",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.test npx prisma migrate reset",
    "seed:dev": "dotenv -e .env.dev npx prisma db seed",
    "seed:test": "dotenv -e .env.test npx prisma db seed"
  },
  "prisma" : {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.13.0",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "^1.3.53",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "delay-cli": "^2.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.13.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  }
